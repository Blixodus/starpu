# Variables d'environnements : 
# ALGO_USED : 	1 pour l'algo qui regroupe les paquets qui atteignent tous la valeurs maximale du poids des données communes. 
#				2 pour l'algo qui fais un regroupement de paquets puis recalcule le maximun du poids.
#				3
# RANDOM_TASK_ORDER : 0 pour ne pas mélanger l'ordre des tâches, 1 pour mélanger.
# STARPU_WORKER_STATS pour un fichier de sortie
# 4 8 12 16 24 32 40 48 pour l'ordre Z

ulimit -S -s 50000
echo "Tests pour algo 1"
STARPU_SCHED=basic-sched ALGO_USED=1 STARPU_LIMIT_BANDWIDTH=500 STARPU_LIMIT_CUDA_MEM=500 RANDOM_TASK_ORDER=0 STARPU_GENERATE_TRACE=0 STARPU_WORKER_STATS=1 STARPU_NCPU=0 STARPU_NCUDA=1 STARPU_NOPENCL=0 STARPU_HOSTNAME=attila ./examples/mult/sgemm -xy $((960*5)) -nblocks 5 -iter 1
STARPU_SCHED=basic-sched ALGO_USED=1 STARPU_LIMIT_BANDWIDTH=500 STARPU_LIMIT_CUDA_MEM=500 RANDOM_TASK_ORDER=0 STARPU_GENERATE_TRACE=0 STARPU_WORKER_STATS=1 STARPU_NCPU=0 STARPU_NCUDA=1 STARPU_NOPENCL=0 STARPU_HOSTNAME=attila ./examples/mult/sgemm -xy $((960*10)) -nblocks 10 -iter 1
STARPU_SCHED=basic-sched ALGO_USED=1 STARPU_LIMIT_BANDWIDTH=500 STARPU_LIMIT_CUDA_MEM=500 RANDOM_TASK_ORDER=0 STARPU_GENERATE_TRACE=0 STARPU_WORKER_STATS=1 STARPU_NCPU=0 STARPU_NCUDA=1 STARPU_NOPENCL=0 STARPU_HOSTNAME=attila ./examples/mult/sgemm -xy $((960*15)) -nblocks 15 -iter 1
STARPU_SCHED=basic-sched ALGO_USED=1 STARPU_LIMIT_BANDWIDTH=500 STARPU_LIMIT_CUDA_MEM=500 RANDOM_TASK_ORDER=0 STARPU_GENERATE_TRACE=0 STARPU_WORKER_STATS=1 STARPU_NCPU=0 STARPU_NCUDA=1 STARPU_NOPENCL=0 STARPU_HOSTNAME=attila ./examples/mult/sgemm -xy $((960*20)) -nblocks 20 -iter 1
STARPU_SCHED=basic-sched ALGO_USED=1 STARPU_LIMIT_BANDWIDTH=500 STARPU_LIMIT_CUDA_MEM=500 RANDOM_TASK_ORDER=0 STARPU_GENERATE_TRACE=0 STARPU_WORKER_STATS=1 STARPU_NCPU=0 STARPU_NCUDA=1 STARPU_NOPENCL=0 STARPU_HOSTNAME=attila ./examples/mult/sgemm -xy $((960*25)) -nblocks 25 -iter 1
STARPU_SCHED=basic-sched ALGO_USED=1 STARPU_LIMIT_BANDWIDTH=500 STARPU_LIMIT_CUDA_MEM=500 RANDOM_TASK_ORDER=0 STARPU_GENERATE_TRACE=0 STARPU_WORKER_STATS=1 STARPU_NCPU=0 STARPU_NCUDA=1 STARPU_NOPENCL=0 STARPU_HOSTNAME=attila ./examples/mult/sgemm -xy $((960*30)) -nblocks 30 -iter 1
STARPU_SCHED=basic-sched ALGO_USED=1 STARPU_LIMIT_BANDWIDTH=500 STARPU_LIMIT_CUDA_MEM=500 RANDOM_TASK_ORDER=0 STARPU_GENERATE_TRACE=0 STARPU_WORKER_STATS=1 STARPU_NCPU=0 STARPU_NCUDA=1 STARPU_NOPENCL=0 STARPU_HOSTNAME=attila ./examples/mult/sgemm -xy $((960*35)) -nblocks 35 -iter 1
STARPU_SCHED=basic-sched ALGO_USED=1 STARPU_LIMIT_BANDWIDTH=500 STARPU_LIMIT_CUDA_MEM=500 RANDOM_TASK_ORDER=0 STARPU_GENERATE_TRACE=0 STARPU_WORKER_STATS=1 STARPU_NCPU=0 STARPU_NCUDA=1 STARPU_NOPENCL=0 STARPU_HOSTNAME=attila ./examples/mult/sgemm -xy $((960*40)) -nblocks 40 -iter 1
STARPU_SCHED=basic-sched ALGO_USED=1 STARPU_LIMIT_BANDWIDTH=500 STARPU_LIMIT_CUDA_MEM=500 RANDOM_TASK_ORDER=0 STARPU_GENERATE_TRACE=0 STARPU_WORKER_STATS=1 STARPU_NCPU=0 STARPU_NCUDA=1 STARPU_NOPENCL=0 STARPU_HOSTNAME=attila ./examples/mult/sgemm -xy $((960*45)) -nblocks 45 -iter 1
STARPU_SCHED=basic-sched ALGO_USED=1 STARPU_LIMIT_BANDWIDTH=500 STARPU_LIMIT_CUDA_MEM=500 RANDOM_TASK_ORDER=0 STARPU_GENERATE_TRACE=0 STARPU_WORKER_STATS=1 STARPU_NCPU=0 STARPU_NCUDA=1 STARPU_NOPENCL=0 STARPU_HOSTNAME=attila ./examples/mult/sgemm -xy $((960*50)) -nblocks 50 -iter 1
